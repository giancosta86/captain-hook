    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.3",
    "@types/react": "^17",
    "@types/react-test-renderer": "^17",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
